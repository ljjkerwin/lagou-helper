{
  "manifest_version": 2,
  "name": "lagou-helper",
  "description": "lagou-helper",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.lagou.com/*"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ]
}
